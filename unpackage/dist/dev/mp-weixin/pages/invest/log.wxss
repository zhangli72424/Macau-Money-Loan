view, text, input {
  box-sizing: border-box;
  line-height: 1;
}
.over-txt {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.hover-transform1 {
  opacity: .95;
  -webkit-transform: scale(0.95);
          transform: scale(0.95);
}
.hover-transform2 {
  background-color: #f6f6f6;
}
.nodata {
  width: 750rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #b9b9b9;
  font-size: 13px;
}
.pagetitle {
  padding: 0 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 20px;
  color: #E9E9F4;
  font-weight: bold;
}
page {
  background-color: #f1f1f1;
}
.app-log .nav-title {
  width: 504rpx;
  line-height: 64rpx;
  height: 64rpx;
  background-color: #E9E9F4;
  border-radius: 10rpx;
  overflow: hidden;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin: 15rpx auto 0;
}
.app-log .nav-title .item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  line-height: 64rpx;
  height: 64rpx;
  font-size: 26rpx;
  color: #878787;
}
.app-log .nav-title .item.active {
  background-color: #009f5e;
  color: #E9E9F4;
  border-radius: 10rpx;
}
.app-log .list .li {
  background-color: #E9E9F4;
  padding: 0 30rpx 50rpx;
  margin: 30rpx;
  border-radius: 16rpx;
}
.app-log .list .li .title {
  border-bottom: 1rpx solid #E2E2E2;
  height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.app-log .list .li .title .txt-title {
  color: #EEA935;
  font-size: 14px;
  font-weight: bold;
}
.app-log .list .li .title .txt-time {
  color: #4FC295;
  font-size: 12px;
  font-weight: 300;
}
.app-log .list .li .content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.app-log .list .li .content .content-item:last-of-type {
  text-align: right;
}
.app-log .list .li .content .content-item .txt {
  margin-top: 20rpx;
  font-size: 13px;
  color: #393939;
}
.app-log .list .li .content .content-item .text-status {
  font-size: 36rpx;
  font-weight: bold;
  margin-top: 15rpx;
  color: #878787;
}
.app-log .list .li .content .content-item .text-status.green {
  color: #009f5e;
}

