view, text, input {
  box-sizing: border-box;
  line-height: 1;
}
.over-txt {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.hover-transform1 {
  opacity: .95;
  -webkit-transform: scale(0.95);
          transform: scale(0.95);
}
.hover-transform2 {
  background-color: #f6f6f6;
}
.nodata {
  width: 750rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #b9b9b9;
  font-size: 13px;
}
.pagetitle {
  padding: 0 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 20px;
  color: #E9E9F4;
  font-weight: bold;
}
.app-aboutus .index-top-nav-title {
  width: 100%;
  font-size: 36rpx;
  text-align: center;
  background: #FFFFFF;
  color: #FFFEFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.app-aboutus .index-top-nav-title .leftIcon {
  width: 80rpx;
}
.app-aboutus .index-top-nav-title .rightIcon {
  width: 80rpx;
}
.app-aboutus .index-top-nav-title .index-top-nav-title-content {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.app-aboutus .content {
  margin: 30rpx;
}
.app-aboutus .content .title {
  padding: 66rpx 0 40rpx 0;
  line-height: 1;
  font-size: 15px;
  font-weight: bold;
  text-align: center;
  color: #FFFEFF;
}
.app-aboutus .content .p {
  color: #999999;
  font-size: 14px;
  line-height: 22px;
  margin-bottom: 30rpx;
  width: 750rpx;
}
.app-aboutus image {
  width: 750rpx !important;
}
.app-aboutus ._img {
  width: 750rpx !important;
}

