.app-login .greeting {
  color: #E9E9F4;
  margin-left: 30rpx;
  margin-top: 20rpx;
  font-weight: bold;
  position: relative;
}
.app-login .greeting .hello {
  font-size: 30px;
  margin-bottom: 16rpx;
}
.app-login .greeting .welcome {
  font-size: 15px;
}
.app-login .greeting .iconfont {
  position: absolute;
  right: 30rpx;
  top: 15rpx;
  font-size: 20px;
  color: #606060;
}
.app-login .account-type {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 30rpx;
  padding: 20rpx 0 20rpx 30rpx;
}
.app-login .account-type .account-type-item {
  margin-right: 40rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #929191;
}
.app-login .account-type .account-type-item.active {
  color: #EEA935;
  font-size: 40rpx;
}
.app-login .content {
  width: 690rpx;
  margin-left: 30rpx;
  margin-top: 20rpx;
  padding: 20rpx 30rpx;
}
.app-login .content .login-title {
  font-size: 14px;
  color: #525252;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
}
.app-login .content .login-title .outer-text {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: inline-block;
  height: 70rpx;
  line-height: 70rpx;
  margin-top: 20rpx;
}
.app-login .content .login-title .outer-text .inner-text {
  display: inline-block;
  width: 100%;
}
.app-login .content .login-title .outer-text .inner-text.active {
  font-size: 15px;
  font-weight: bold;
  color: #EEA935;
}
.app-login .content .login-title .outer-text:last-child .inner-text {
  border-left: 1rpx solid #4FC295;
}
.app-login .content .li.state {
  height: 84rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #E9E9F4;
  font-size: 14px;
  padding-left: 40rpx;
  margin-top: 10rpx;
}
.app-login .content .li.state .left {
  height: 100%;
  line-height: 84rpx;
  margin-right: 30rpx;
  margin-left: 10rpx;
}
.app-login .content .li.state .center {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 10px;
}
.app-login .content .li.state .right {
  width: 100rpx;
  text-align: right;
  height: 100%;
  line-height: 80rpx;
}
.app-login .content .li {
  height: 84rpx;
  border-bottom: 1rpx solid #929191;
  position: relative;
  z-index: 1;
  padding-left: 40rpx;
}
.app-login .content .li input {
  height: 84rpx;
  font-size: 12px;
  color: #E9E9F4;
}
.app-login .content .li .v-code {
  position: absolute;
  right: 40rpx;
  top: 0;
  height: 84rpx;
  color: #EEA935;
  font-weight: bold;
  line-height: 84rpx;
  font-size: 13px;
  padding: 0 10rpx;
  width: 100px;
  text-align: right;
  z-index: 10;
}
.app-login .content .li .iconfont {
  position: absolute;
  right: 40rpx;
  top: 0;
  width: 80rpx;
  height: 84rpx;
  line-height: 84rpx;
  z-index: 5;
  text-align: right;
  color: #EEA935;
}
.app-login .content .li + .li {
  margin-top: 30rpx;
}
.app-login .content .btn-wrapper {
  margin-top: 50rpx;
}
.app-login .content .btn-wrapper .sell-btn {
  margin-top: 120rpx;
  margin: 128rpx 30rpx 0;
  line-height: 88rpx;
  height: 88rpx;
  border-radius: 6rpx;
  background: #929292;
  color: #E9E9F4;
}
.app-login .content .btn-wrapper .sell-btn.active {
  background: -webkit-linear-gradient(top, #d4ae6d, #fedf9d);
  background: linear-gradient(to bottom, #d4ae6d, #fedf9d);
  color: #212121;
  box-shadow: 0 0 10rpx rgba(251, 217, 148, 0.2);
}
.app-login .content .tip {
  font-size: 11px;
  color: #929292;
  margin-bottom: 80rpx;
  margin-top: 26rpx;
}
.app-login .goLogin {
  width: 690rpx;
  height: 86rpx;
  color: #EEA935;
  line-height: 86rpx;
  text-align: center;
  font-size: 13px;
  margin-left: 30rpx;
}

